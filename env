# C:\Users\$USER\AppData\Local\Julia-0.4.0-dev\bin\julia.exe

cyu="/cygdrive/c/Users/$USER"

if [ -d "$cyu" ] ; then
   PATH=$cyu/AppData/Local/Julia-0.4.0/bin:"$PATH"
else
   PATH=/Applications/Julia-0.4.5.app/Contents/Resources/julia/bin/julia:$PATH
#/Volumes/Julia/Julia-0.4.5.app/Contents/Resources/julia/bin:$PATH
fi
